#include <iostream>
#include <string>
/* Lexicographic order permutation */
sring getPermutation(int n, int k) {
  string s(n, '0');
  string result;
  for (int i = 0; i < n; i++)
  {
    s[i] += i + 1;
  }

  return kth_permutation(s, k);
}

int factorial(int n) {
  int result = 1;
  for (int i = 1; i <= n; i++) {
    result *= i;
  }
  return result;
}

